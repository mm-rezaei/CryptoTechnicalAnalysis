                                            <dxg:GridControl.Columns>
                                                <dxg:GridColumn Width="80" Header="Name" FieldName="Name" />
                                                <dxg:GridColumn Width="80" Header="TimeFrame" FieldName="TimeFrame">
                                                    <dxg:GridColumn.DisplayTemplate>
                                                        <ControlTemplate>
                                                            <TextBlock Text="{Binding Value}" />
                                                        </ControlTemplate>
                                                    </dxg:GridColumn.DisplayTemplate>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn Width="80" Header="Distance" FieldName="Percent">
                                                    <dxg:GridColumn.DisplayTemplate>
                                                        <ControlTemplate>
                                                            <TextBlock Text="{Binding Value,  Converter={StaticResource  PercentFloatNumberToPercentStringConverter}}" />
                                                        </ControlTemplate>
                                                    </dxg:GridColumn.DisplayTemplate>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn Header="Price" FieldName="Price">
                                                    <dxg:GridColumn.DisplayTemplate>
                                                        <ControlTemplate>
                                                            <TextBlock Text="{Binding Value,  Converter={StaticResource  FloatNumberToStringConverter}}" />
                                                        </ControlTemplate>
                                                    </dxg:GridColumn.DisplayTemplate>
                                                </dxg:GridColumn>
                                            </dxg:GridControl.Columns>
                                        </dxg:GridControl>
                                    </dxg:DataControlDetailDescriptor.DataControl>
                                </dxg:DataControlDetailDescriptor>
                            </dxg:TabViewDetailDescriptor.DetailDescriptors>
                        </dxg:TabViewDetailDescriptor>
                    </dxg:GridControl.DetailDescriptor>
                </dxg:GridControl>
            </Grid>
            <Grid  Grid.Row="1" Background="LightGray">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="120" />
                    <ColumnDefinition Width="120" />
                    <ColumnDefinition Width="28" />
                    <ColumnDefinition Width="185" />
                </Grid.ColumnDefinitions>
                <StatusBar Grid.Column="0" Padding="4 0 0 0" Background="LightGray">
                    <Ellipse Name="EllipseServiceActivation" Width="12" Height="12" Fill="Red" Canvas.Left="135" Canvas.Top="25"/>
                </StatusBar>
                <Border Grid.Column="1" BorderThickness="1 0 0 0" BorderBrush="Gray">
                    <TextBlock Name="TextBlockServerConnectionStatus" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </Border>
                <Border Grid.Column="2" BorderThickness="1 0 0 0" BorderBrush="Gray">
                    <TextBlock Name="TextBlockSymbolSync" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </Border>
                <Border Grid.Column="3" BorderThickness="1 0 0 0" BorderBrush="Gray">
                    <Image Name="ImageSound" Margin="4 0 0 0" Source="../Images/Unmute.png" Width="16" Height="16" Cursor="Hand" MouseDown="ImageSound_MouseDown" />
                </Border>
                <Border Grid.Column="4" BorderThickness="0 0 0 0" BorderBrush="Gray">
                    <TextBlock Name="TextBlockCurrentTime" VerticalAlignment="Center" HorizontalAlignment="Center" />
                </Border>
            </Grid>
        </Grid>
    </Grid>
</Window>
