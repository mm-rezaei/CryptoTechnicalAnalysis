<Window x:Class="TechnicalAnalysisTools.Ui.Win.Forms.MainWindow"
        x:ClassModifier="internal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
        xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
        xmlns:convertors="clr-namespace:TechnicalAnalysisTools.Shared.ValueConverters;assembly=TechnicalAnalysisTools.Shared"
        mc:Ignorable="d"
        Name="WindowMain"
        Icon="../Images/Binance.png"
        Loaded="Window_Loaded"
        Closing="Window_Closing"
        Closed="Window_Closed"
        WindowStartupLocation="CenterScreen"
        Title="Chanel Trading Client" Height="450" Width="800" WindowState="Maximized">
    <Window.Resources>
        <Style x:Key="MainViewRowStyle" TargetType="dxg:RowControl">
            <Setter Property="Background" Value="RoyalBlue" />
            <Setter Property="Foreground" Value="White" />
            <Setter Property="FontSize" Value="12" />
        </Style>
        <Style x:Key="AlarmViewRowStyle" TargetType="dxg:RowControl">
            <Setter Property="Foreground" Value="{Binding DataContext.AlarmForgroundColor}" />
            <Setter Property="Background" Value="{Binding DataContext.AlarmBackgroundColor}" />
        </Style>
        <Style x:Key="SupportResistanceViewRowStyle" TargetType="dxg:RowControl">
            <Setter Property="Foreground" Value="{Binding DataContext.SupportResistanceForgroundColor}" />
            <Setter Property="Background" Value="{Binding DataContext.SupportResistanceBackgroundColor}" />
        </Style>
        <convertors:DateTimeToStringConverter x:Key="DateTimeToStringConverter" />
        <convertors:SymbolIsAscendingValueToBrushConverter x:Key="SymbolIsAscendingValueToBrushConverter" />
        <convertors:TimeFrameIsAscendingValueToBrushConverter x:Key="TimeFrameIsAscendingValueToBrushConverter" />
        <convertors:DivergenceValueToBrushConverter x:Key="DivergenceValueToBrushConverter" />
        <convertors:PercentFloatNumberToPercentStringConverter x:Key="PercentFloatNumberToPercentStringConverter" />
        <convertors:FloatNumberToStringConverter x:Key="FloatNumberToStringConverter" />
    </Window.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Menu Name="MenuMain" Grid.Row="0" Height="24" Padding="3">
            <MenuItem Header="_File">
                <MenuItem Name="MenuItemStart" Header="_Start" IsEnabled="False" Click="MenuItemStart_Click" />
                <Separator />
                <MenuItem Name="MenuItemLiveHistory" Header="Show Live _History" IsEnabled="False" Click="MenuItemLiveHistory_Click" />
                <Separator />
                <MenuItem Name="MenuItemExit" Header="_Exit" Click="MenuItemExit_Click" />
            </MenuItem>
            <MenuItem Header="_Alarm">
                <MenuItem Name="MenuItemShowAlarmWindow" Header="Show Alarm _Window" IsEnabled="False" Click="MenuItemShowAlarmWindow_Click" />
                <MenuItem Name="MenuItemShowAlarmHistoryWindow" Header="Show Alarm _History" IsEnabled="False" Click="MenuItemShowAlarmHistoryWindow_Click" />
                <Separator />
                <MenuItem Name="MenuItemNewAlarmScript" Header="_New Alarm" IsEnabled="False" Click="MenuItemNewAlarmScript_Click" />
                <MenuItem Name="MenuItemEditAlarmScript" Header="_Edit Alarm" IsEnabled="False" Click="MenuItemEditAlarmScript_Click" />
                <Separator />
                <MenuItem Name="MenuItemRunAlarmScripts" Header="_Run Alarms" IsEnabled="False" Click="MenuItemRunAlarmScripts_Click" />
                <MenuItem Name="MenuItemRunTemplateAlarmScript" Header="Run _Template Alarm" IsEnabled="False" Click="MenuItemRunTemplateAlarmScript_Click" />
            </MenuItem>
            <MenuItem Header="_Strategy">
                <MenuItem Name="MenuItemBackTestNewStrategy" Header="Test _New Strategy" IsEnabled="False" Click="MenuItemBackTestNewStrategy_Click" />
            </MenuItem>
        </Menu>
        <Grid Grid.Row="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="28"/>
            </Grid.RowDefinitions>
            <Grid Name="GridMain" Grid.Row="0">
                <Grid Name="GridLoadingData" Margin="100 8" VerticalAlignment="Center" Visibility="Collapsed">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="28" />
                        <RowDefinition Height="28" />
                        <RowDefinition Height="28" />
                        <RowDefinition Height="28" />
                    </Grid.RowDefinitions>
                    <TextBlock Name="TextBlockSymbolDataLoadingMessage" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Center">The Symbol data set is loading...</TextBlock>
                    <ProgressBar Name="ProgressBarSymbolDataLoading" Grid.Row="1" Minimum="0" Maximum="100"></ProgressBar>
                    <TextBlock Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center">All supported data sets are loading. Please wait...</TextBlock>
                    <ProgressBar Name="ProgressBarAllDataLoading" Grid.Row="3" Minimum="0" Maximum="100"></ProgressBar>
                </Grid>
                <dxg:GridControl x:Name="GridControlMain" Visibility="Collapsed">
                    <dxg:GridControl.View>
                        <dxg:TableView AutoWidth="True" ShowGroupPanel="False" AllowEditing="False" NavigationStyle="Row" DetailHeaderContent="Candles" RowStyle="{StaticResource MainViewRowStyle}" />
                    </dxg:GridControl.View>
                    <dxg:GridControl.Columns>
                        <dxg:GridColumn Width="14" Header="" AllowResizing="False" AllowAutoFilter="False" AllowColumnFiltering="False" AllowSorting="False" FieldName="IsAscending">
                            <dxg:GridColumn.DisplayTemplate>
                                <ControlTemplate>
                                    <Border Width="8" Background="{Binding Value,  Converter={StaticResource  SymbolIsAscendingValueToBrushConverter}}" />
                                </ControlTemplate>
                            </dxg:GridColumn.DisplayTemplate>
                        </dxg:GridColumn>
                        <dxg:GridColumn Width="50" Header="%" FieldName="ChangedPercent">
                            <dxg:GridColumn.DisplayTemplate>
                                <ControlTemplate>
                                    <TextBlock Text="{Binding Value,  Converter={StaticResource  PercentFloatNumberToPercentStringConverter}}" />
                                </ControlTemplate>
                            </dxg:GridColumn.DisplayTemplate>
                        </dxg:GridColumn>
                        <dxg:GridColumn Header="Symbol" FieldName="Symbol" />
                        <dxg:GridColumn Header="Last Update" FieldName="LastMinuteCandle">
                            <dxg:GridColumn.DisplayTemplate>
                                <ControlTemplate>
                                    <TextBlock Margin="3 0 0 0"  Text="{Binding Value,  Converter={StaticResource  DateTimeToStringConverter}}"/>
                                </ControlTemplate>
                            </dxg:GridColumn.DisplayTemplate>
                        </dxg:GridColumn>
                        <dxg:GridColumn Header="Type" FieldName="CandleType" />
                        <dxg:GridColumn Header="Open" FieldName="Open">
                            <dxg:GridColumn.DisplayTemplate>
                                <ControlTemplate>
                                    <TextBlock Text="{Binding Value,  Converter={StaticResource  FloatNumberToStringConverter}}" />
                                </ControlTemplate>
                            </dxg:GridColumn.DisplayTemplate>
                        </dxg:GridColumn>
                        <dxg:GridColumn Header="Close" FieldName="Close">
                            <dxg:GridColumn.DisplayTemplate>
                                <ControlTemplate>
                                    <TextBlock Text="{Binding Value,  Converter={StaticResource  FloatNumberToStringConverter}}" />
                                </ControlTemplate>
                            </dxg:GridColumn.DisplayTemplate>
                        </dxg:GridColumn>
                        <dxg:GridColumn Header="High" FieldName="High">
                            <dxg:GridColumn.DisplayTemplate>
                                <ControlTemplate>
                                    <TextBlock Text="{Binding Value,  Converter={StaticResource  FloatNumberToStringConverter}}" />
                                </ControlTemplate>
                            </dxg:GridColumn.DisplayTemplate>
                        </dxg:GridColumn>
                        <dxg:GridColumn Header="Low" FieldName="Low">
                            <dxg:GridColumn.DisplayTemplate>
                                <ControlTemplate>
                                    <TextBlock Text="{Binding Value,  Converter={StaticResource  FloatNumberToStringConverter}}" />
