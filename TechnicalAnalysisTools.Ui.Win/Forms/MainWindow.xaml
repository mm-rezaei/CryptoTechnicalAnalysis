                                                <dxg:TableView AutoWidth="True" ShowGroupPanel="False" AllowEditing="False" NavigationStyle="Row" AllowSorting="False" DetailHeaderContent="Supports/Resistances" RowStyle="{StaticResource SupportResistanceViewRowStyle}" />
